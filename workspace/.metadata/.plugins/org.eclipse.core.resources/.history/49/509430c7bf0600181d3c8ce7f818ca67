//import java.util.Arrays;

public class Student extends CartoonFigure{
	public Student(String _Name, String _Talent, int _Id){

		name = _Name;
		talent = _Talent;
		Id = _Id;
	}

	public void Puts(){
		for (int i=0; i<10; i++){
			System.out.println(name);
			System.out.println(talent);
			System.out.println(Id);
		}

	}
}
