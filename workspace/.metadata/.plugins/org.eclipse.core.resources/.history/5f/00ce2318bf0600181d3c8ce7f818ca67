package project2;

public class Point {
	//this is where some stuff will go.

	//default constructor
	public Point(){
		x = 0;
		y = 0;

	}

	//another constructor with the same name
	public Point(int _x, int _y){
		x = _x;
		y = _y;
	}

	//data members (not starfleet)
	public int x;
	public int y;

}
